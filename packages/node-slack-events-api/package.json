{
  "name": "@slack/events-api",
  "version": "1.0.1",
  "description": "Slack Events API module",
  "main": "dist/index.js",
  "bin": {
    "slack-verify": "dist/verify.js"
  },
  "repository": "https://github.com/slackapi/node-slack-events-api.git",
  "engines": {
    "node": ">=4.2.0"
  },
  "files": [
    "dist/**/*"
  ],
