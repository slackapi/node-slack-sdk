{
  "name": "@slack/events-api",
  "version": "2.3.0",
  "description": "Official library for using the Slack Platform's Web API",
  "author": "Slack Technologies, Inc.",
  "license": "MIT",
  "keywords": [
    "slack",
    "events-api",
    "bot",
    "server",
    "http",
    "api",
    "verify",
    "signature",
    "request-signing",
    "event-emitter"
  ],
  "main": "dist/index.js",
  "bin": {
    "slack-verify": "dist/verify.js"
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=4.2.0"
  },
  "repository": "slackapi/node-slack-sdk",
  "homepage": "https://slack.dev/node-slack-sdk/events-api",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/slackapi/node-slack-sdk/issues"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:clean && tsc",
    "build:clean": "shx rm -rf ./dist ./coverage ./.nyc_output",
    "test": "nyc mocha src/*.spec.js test/integration/*.js",
    "coverage": "codecov -F eventsapi --root=$PWD"
  },
  "dependencies": {
    "@types/debug": "^4.1.4",
    "@types/express": "^4.17.0",
    "@types/lodash.isstring": "^4.0.6",
    "@types/yargs": "^13.0.0",
    "debug": "^2.6.1",
    "lodash.isstring": "^4.0.1",
    "raw-body": "^2.3.3",
    "tsscmp": "^1.0.6",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.8",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^7.0.13",
    "chai": "^4.2.0",
    "codecov": "^3.0.4",
    "express": "^4.14.0",
    "get-random-port": "0.0.1",
    "lodash.isfunction": "^3.0.8",
    "mocha": "^6.1.4",
    "nop": "^1.0.0",
    "nyc": "^14.1.1",
    "proxyquire": "^1.7.10",
    "shx": "^0.3.2",
    "sinon": "^4.5.0",
    "source-map-support": "^0.5.12",
    "superagent": "^3.3.1",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1",
    "uncaughtException": "^1.0.0"
  },
  "optionalDependencies": {
    "express": "^4.0.0"
  }
}
